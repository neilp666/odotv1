<h1>Log in</h1>

<%= form_tag user_sessions_path do %>
  <%= label_tag :email, "Email Address" %>
  <%= text_field_tag :email, params[:email] %>
  <br />


  <%= label_tag :password, "Password" %>
  <%= password_field_tag :password %>
  <br />

  <%= submit_tag "Log in" %>
  <%= link_to "Forgot Password", new_password_reset_path %>
<% end %>
